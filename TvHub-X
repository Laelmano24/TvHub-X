local Fluent = loadstring(game:HttpGet("https://raw.githubusercontent.com/RileyBeeRBLX4/UI-Library/refs/heads/main/Fluent%20UI/Library.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

if not Fluent then return end

local Window = Fluent:CreateWindow({
    Title = "TvHub" .. Fluent.Version,
    SubTitle = "by TvZone",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local MainTab = Window:AddTab({ Title = "Book 1", Icon = "" })

local Section1 = MainTab:AddSection("B1c2")

MainTab:AddButton({
    Title = "AutoPass",
    Description = "Auto passes part 1",
    Callback = function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(65.12, 56.40, -1584.93)
        end
    end
})

MainTab:AddButton({
    Title = "Part 2 Auto Pass",
    Description = "Very important button",
    Callback = function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(236.37, 101.94, -588.19)
        end
    end
})

MainTab:AddButton({
    Title = "Part 3 Auto Pass",
    Description = "Very important button",
    Callback = function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(906.88, 72.92, -354.89)
        end
    end
})

local Section2 = MainTab:AddSection("B1C3")

MainTab:AddButton({
    Title = "1 auto pass",
    Description = "Self Explanatory",
    Callback = function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2411.82, -25.03, 2276.95)
        end
    end
})

MainTab:AddButton({
    Title = "Part 2 Auto Pass",
    Description = "Self Explanatory",
    Callback = function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(240.75, 31.71, 440.66)
        end
    end
})

MainTab:AddButton({
    Title = "Part 3 Auto Pass",
    Description = "Self Explanatory",
    Callback = function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-625.29, 947.80, -1368.49)
        end
    end
})

local OtherTab = Window:AddTab({ Title = "Other", Icon = "" })

OtherTab:AddButton({
    Title = "FullBright",
    Description = "Very important button",
    Callback = function()
        local Light = game:GetService("Lighting")
        if Light then
            Light.Ambient = Color3.new(1, 1, 1)
            Light.ColorShift_Bottom = Color3.new(1, 1, 1)
            Light.ColorShift_Top = Color3.new(1, 1, 1)
            Light.LightingChanged:Connect(function()
                Light.Ambient = Color3.new(1, 1, 1)
                Light.ColorShift_Bottom = Color3.new(1, 1, 1)
                Light.ColorShift_Top = Color3.new(1, 1, 1)
            end)
        end
    end
})

OtherTab:AddButton({
    Title = "Esp (Players Only)",
    Description = "Players only",
    Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/Lucasfin000/SpaceHub/main/UESP'))()
    end
})
